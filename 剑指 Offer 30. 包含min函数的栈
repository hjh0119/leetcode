class MinStack {
private:
    stack<int> s;
    stack<int> mins;
public:
    /** initialize your data structure here. */
    MinStack() {
        mins.push(INT_MAX);
    }
    
    void push(int x) {
        s.push(x);
        mins.push(::min(x,mins.top()));
    }
    
    void pop() {
        if(s.empty())return;
        s.pop();
        mins.pop();
    }
    
    int top() {
        return s.top();
    }
    
    int min() {
        returnã€€mins.top();
    }
};

